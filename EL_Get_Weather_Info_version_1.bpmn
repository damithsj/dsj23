    <bpmn:scriptTask id="Activity_07m74xt" name="Get Lat Lon" scriptFormat="JavaScript">
      <bpmn:incoming>Flow_02h20h5</bpmn:incoming>
      <bpmn:outgoing>Flow_17ksequ</bpmn:outgoing>
      <bpmn:script>var coordsArr = execution.getVariable("GetObjConnResultSet");
var coords= coordsArr [0];

execution.setVariable('varLat', coords.Latitude);
execution.setVariable('varLon', coords.Longitude);</bpmn:script>
    </bpmn:scriptTask>
