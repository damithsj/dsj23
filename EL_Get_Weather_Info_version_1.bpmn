          <camunda:inputParameter name="ifsBpaRestResponseVariableName">weatherResponse</camunda:inputParameter>
          <camunda:inputParameter name="ifsBpaRestResponseCheck" />
          <camunda:inputParameter name="ifsBpaHttpConnectionOutputs">
            <camunda:map>
              <camunda:entry key="Output">$.properties.timeseries</camunda:entry>
            </camunda:map>
          </camunda:inputParameter>
          <camunda:inputParameter name="ifsBpaHttpConnectionHeaders">
            <camunda:map>
              <camunda:entry key="Accept">application/json</camunda:entry>
            </camunda:map>
          </camunda:inputParameter>
          <camunda:inputParameter name="ifsBpaUserHandleResponseCode">301,400,404</camunda:inputParameter>
        </camunda:inputOutput>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_0g5ci4o</bpmn:incoming>
      <bpmn:outgoing>Flow_0iot9wx</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="Flow_10470xc" sourceRef="StartEvent_1" targetRef="Activity_0hvg171" />
    <bpmn:sequenceFlow id="Flow_17ksequ" sourceRef="Activity_07m74xt" targetRef="Activity_18lj125" />
    <bpmn:serviceTask id="Activity_18lj125" name="Get Location Data" camunda:class="com.ifsworld.fnd.bpa.delegate.IfsHttpConnectionDelegate">
